{
  "expo": {
    "name": "shreddersbay",
    "slug": "shreddersbay",
    "version": "1.0.13",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "userInterfaceStyle": "light",
    "platforms": [
      "android",
      "ios"
    ],
    "permissions": [
      "INTERNET"
    ],
    "extra": {
      "eas": {
        "projectId": "899a3d51-4ab2-46d5-a140-644e3ad1b7be"
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#abcdef",
      "barStyle": "dark-content",
      "translucent": false,
      "hidden": false
    },
    "splash": {
      "image": "./assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shreddersbay",
      "minVersion": "13.0",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["second_project"]
          }
        ]
      },
      "statusBar": {
        "backgroundColor": "#abcdef",
        "style": "auto"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shreddersbay",
      "versionCode": 13,
      "googleServicesFile": "./google-services.json",
      "expo.android.googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "vinitrathore",
    "scheme": "shreddersbay", 
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "fallbackToCacheTimeout": 10000,
      "url": "https://u.expo.dev/899a3d51-4ab2-46d5-a140-644e3ad1b7be"
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          },
          "ios": {
            "deploymentTarget": "13.4",
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-updates",
        {
          "username":"vinitrathore"
        }
      ]
    ]
  }
}

